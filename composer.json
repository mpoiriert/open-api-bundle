{
    "name": "draw/open-api-bundle",
    "type": "symfony-bundle",
    "description": "draw/open-api library integration into Symfony4.",
    "minimum-stability": "dev",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Poirier-Theoret",
            "email": "mpoiriert@gmail.com"
        }
    ],
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Draw\\Bundle\\OpenApiBundle\\": ""
        }
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "draw/open-api": "^0.5.16",
        "doctrine/annotations": "~1.0",
        "symfony/framework-bundle": "^4.3",
        "jms/serializer-bundle": "@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "sensio/framework-extra-bundle": "^5.0",
        "draw/tester": "^0.5.16"
    },
    "suggest": {
        "nelmio/cors-bundle": "If you have a api exposed for browser on different domain/sub-domain you might need cors."
    }
}
